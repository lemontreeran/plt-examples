{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "description": "<%= appName %>",
  "author": "wxyyxc1992",
  "license": "MIT",
  "homepage": "https://github.com/wxyyxc1992/create-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/create-react-boilerplate"
  },
  "keywords": [
    "boilerplate",
    "live",
    "hot",
    "reload",
    "react",
    "reactjs",
    "hmr",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "PostCSS(FlexBox Polyfill)"
  ],
  "main": "index.js",
  "bin": {
    "create-react-boilerplate": "./index.js"
  },
  "babel": {
    "extends": "./dev-config/.babelrc"
  },
  "scripts": {
    "start": "webpack-dashboard -- node --harmony ./dev-config/server/devServer.js --host 0.0.0.0",
    "build": "npm run clean && better-npm-run build",
    "build:ssr": "npm run clean && better-npm-run build:ssr && better-npm-run build:server",
    "build:library": "npm run clean && better-npm-run build:library",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "mocha": "./node_modules/mocha/bin/mocha -t 10000 --compilers js:babel-core/register",
    "mocha-all": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive src/**/*.test.js",
    "update": "npm-check -u",
    "weapp": "better-npm-run weapp",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "betterScripts": {
    "build": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:ssr": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.config.js",
      "env": {
        "NODE_ENV": "production",
        "TARGET": "ssr"
      }
    },
    "build:server": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.server.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:library": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.config.js",
      "env": {
        "NODE_ENV": "library"
      }
    },
    "weapp": {
      "command": "./node_modules/.bin/webpack --config ./dev-config/webpack.weapp.config.js --watch",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^3.2.0",
    "imagemin": "^5.2.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "mocha": "^3.0.2",
    "nock": "^9.0.2",
    "node-sass": "^4.3.0",
    "object-assign": "^4.0.1",
    "postcss-flexibility": "^1.1.0",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-komposer": "^2.0.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.2",
    "scss-loader": "^0.0.1",
    "stack-source-map": "^1.0.5",
    "style-loader": "^0.13.0",
    "underscore-template-loader": "^0.7.2",
    "url-loader": "^0.5.6",
    "webpack": "^2.2.1",
